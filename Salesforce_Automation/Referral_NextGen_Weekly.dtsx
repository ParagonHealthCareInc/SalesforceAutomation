            <component
              refId="Package\SF Referral Load\NG Data Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="NG Data Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE
	@start DATETIME;
DECLARE
	@end DATETIME;

--Previous Week Automation
SET @start = DATEADD(dd, -1, DATEADD(ww, DATEDIFF(ww, 0, '04-25-2021') - 1, 0));
SET @end = DATEADD(dd, 5, DATEADD(ww, DATEDIFF(ww, 0, '04-25-2021') - 1, 0));
 


SELECT
	po.seq_no AS Referral_ID__c
   ,UPPER(po.person_id) AS Patient__c
   ,UPPER(CONCAT(LTRIM(RTRIM(p.first_name)), ' ', LTRIM(RTRIM(p.last_name)))) AS Name
   ,ISNULL(CONVERT(DATE, CAST(po.create_timestamp AS DATE), 101), '') AS Last_Referral_Date__c
   ,po.Order_Status AS Status__c
   , LTRIM(RTRIM(pm.national_provider_id)) Prescriber__c
   ,po.Order_Type AS Therapy__c
	, isnull(mx.saleslevel,'Other') NG_Saleslevel
	, Case when Isnull(mx.SalesLevel,'Other') in ('Level 1','Level 2') then 'Specialty Therapy' else 'Non Specialty Therapy' End [Therapy Type]
	, case when ltrim(rtrim(mx.saleslevel)) in ('Level 1','Level 2') then 'L1/L2' else 'Others' end [Therapy Level]
	, Case when sp.Sales_Rep_IVC__c IS NULL THEN '0051I000002GjcjQAC' ELSE sp.Sales_Rep_IVC__c end Sales_Rep_IVC__c

	, isnull(SUBSTRING(loc.external_id, CHARINDEX('-', loc.external_id) + 1, LEN(loc.external_id)), 'Not Assigned') Paragon_Site__c
  ,'NextGen' AS EMR_System__c
  , concat('NG',' ',ltrim(rtrim(po.seq_no))) NG_ID
   ,CASE
		WHEN NOT EXISTS (SELECT * FROM PIVC_Orders_ ou
				WHERE ou.seq_no &lt;&gt; po.seq_no
				AND ou.person_id = po.person_id
				AND ou.Order_Type = po.Order_Type
				AND ou.Ordering_Provider_id = po.Ordering_Provider_id)
			THEN 1
		ELSE 0
	END AS NewOrder__c
FROM PIVC_Orders_ po
INNER JOIN person p  ON po.person_id = p.person_id
INNER JOIN provider_mstr pm ON po.Ordering_Provider_id = pm.provider_id
left join Location_mstr loc with (nolock) on ltrim(rtrim(loc.location_name))=ltrim(rtrim(po.service_location))
inner join [DWDBHA].SALESFORCE.Staging.Prescribers sp on sp.NPI__c=ltrim(rtrim(pm.national_provider_id))
--inner join [DWDBHA].[XREFDATA].[dbo].XrefTerritoryMgmt xm on xm.ivcterritory=sp.ivc_territory__c and xm.zipcode=sp.Billingpostalcode__c
--inner join [DWDBHA].SALESFORCE.Staging.[User] u on u.Name=xm.Salesrep and u.Isactive=1
outer apply (SELECT TOP 1 ISNULL(SalesLevel, 'Other') Saleslevel FROM MLTS_MedInfoXP_	WHERE ActiveInd = 'Y' AND DrugName = po.Medication1 ORDER BY ISNULL(SalesLevel, '') DESC) MX

WHERE po.person_id NOT IN ('EF477608-5F49-4159-A405-DB07E13EE7E4', '1EAA6B96-D42A-454F-86B4-BC669B3F6DFE', '1018C999-1B03-4B98-9A83-3FE166776450', '658921E3-C41F-46FE-A77C-B215A0F2CC75', 'D1C7E60B-F23F-477E-BCB4-7C3A2681B2EC', 'DCAF9281-84BC-4547-ABEC-68F68699110C')
AND (po.modify_timestamp &gt;= @start
AND po.modify_timestamp &lt;= @end)

--AND (po.create_timestamp &gt;= @start
--AND po.create_timestamp &lt;= @end)

ORDER BY Paragon_Site__c</property>
