            <component
              refId="Package\Data Flow Task\ADO NET Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE
	@start DATETIME;
DECLARE
	@end DATETIME;

--Previous Month
SET @start = DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()) - 1, 0);
SET @end = DATEADD(MONTH, DATEDIFF(MONTH, -1, GETDATE()) - 1, -1);

--SET @start = '2020-07-01';
--SET @end = '2020-07-31';


SELECT
	b.CPK_BILLED AS 'Name'
   ,b.BILLNO AS 'Invoice_Number__c'
   ,CONVERT(DATE, b.DATEBILLED, 120) AS 'Date_Billed__c'
   ,LTRIM(RTRIM(h.MRN)) AS 'Patient__c'
   ,
	--concat(ltrim(rtrim(h.MRN)), ' : ', convert(varchar, h.REF_DATE, 111)) Referral_ID__c,
	--b.FROM_, 
	--b.DELFLAG, 
	CASE
		WHEN b.ALLOWED IS NULL
			THEN 0
		ELSE b.ALLOWED
	END AS 'Gross_Revenue__c'
   ,CASE
		WHEN b.ADJUSTAMT IS NULL
			THEN 0
		ELSE b.ADJUSTAMT
	END AS 'Adjustments__c'
   ,((CASE
		WHEN b.ALLOWED IS NULL
			THEN 0
		ELSE b.ALLOWED
	END) -
	(CASE
		WHEN b.ADJUSTAMT IS NULL
			THEN 0
		ELSE b.ADJUSTAMT
	END)) AS 'Adjusted_Revenue__c'
   , --Check for nulls, then (Allowed - ADJUSTAMT)
	CASE
		WHEN b.COST IS NULL
			THEN 0
		ELSE b.COST
	END AS 'COGS__c'
   ,(((CASE
		WHEN b.ALLOWED IS NULL
			THEN 0
		ELSE b.ALLOWED
	END) -
	(CASE
		WHEN b.ADJUSTAMT IS NULL
			THEN 0
		ELSE b.ADJUSTAMT
	END)) - (CASE
		WHEN b.COST IS NULL
			THEN 0
		ELSE b.COST
	END)) AS 'Gross_Profit__c'
   , --Check for nulls, then (Adjusted Revenue - Cost)
	CASE
		WHEN b.PAYREC IS NULL
			THEN 0
		ELSE b.PAYREC
	END AS 'Payments__c'
   ,CASE
		WHEN LEN(LTRIM(RTRIM(h.SALESCODE))) &gt; 0
			THEN LTRIM(RTRIM(h.SALESCODE))
		ELSE 'DHA'
	END AS 'Original_Sales_Code__c'
   ,CASE
		WHEN LEN(LTRIM(RTRIM(h.SALESCODE))) &gt; 0 AND
			EXISTS (SELECT
					*
				FROM (SELECT
						b.SALESCODE
					FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps] b
					WHERE b.Active = '1'
					AND b.SALESCODE IS NOT NULL) sr
				WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
			THEN CASE
					WHEN LTRIM(RTRIM(h.SALESREP)) LIKE '%House%'
						THEN 'ACCT HOUSE'
					ELSE (SELECT
								sr.New_Rep
							FROM (SELECT
									b.New_Rep
								   ,b.SALESCODE
								FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps] b
								WHERE b.Active = '1'
								AND b.SALESCODE IS NOT NULL) sr
							WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
				END
		ELSE 'ACCT HOUSE'
	END AS 'Sales_Rep__c'
   ,CASE
		WHEN LEN(LTRIM(RTRIM(h.SALESCODE))) &gt; 0 AND
			EXISTS (SELECT
					*
				FROM (SELECT
						b.SALESCODE
					FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps] b
					WHERE b.Active = '1'
					AND b.SALESCODE IS NOT NULL) sr
				WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
			THEN (SELECT
						sr.Division
					FROM (SELECT
							b.Division
						   ,b.SALESCODE
						FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps] b
						WHERE b.Active = '1') sr
					WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
		ELSE 'HIT'
	END AS 'Division__c'
   ,LTRIM(RTRIM(s.SITENAME)) AS 'Paragon_Site__c'
   ,'CPR+' AS 'EMR_System__c'
FROM BILLED b (nolock)
INNER JOIN HR h (nolock)
	ON b.MRN = h.MRN
LEFT JOIN SITES s (nolock)
	ON h.SITENO = s.SITENO
WHERE b.DELFLAG = 0
AND h.DELFLAG = 0
AND UPPER(CONCAT(LTRIM(RTRIM(h.FIRST_NAME)), ' ', LTRIM(RTRIM(h.LAST_NAME)))) NOT LIKE '%TEST%'
AND h.REF_DATE IS NOT NULL
AND LEN(LTRIM(RTRIM(h.REF_DATE))) &gt; 1
AND b.DATEBILLED &gt;= @start
AND b.DATEBILLED &lt;= @end
AND (b.DESCRIPT = 'ADJ'
OR b.DESCRIPT = 'CHK'
OR b.DESCRIPT = 'INV')
AND NOT (b.ADJTYPE = 'BAD'
OR b.ADJTYPE = 'BADSW'
OR b.ADJTYPE = 'COAG'
OR b.ADJTYPE = 'CPIRF'
OR b.ADJTYPE = 'INNOV'
OR b.ADJTYPE = 'MEDWY'
OR b.ADJTYPE = 'REFPD'
OR b.ADJTYPE = 'REINS'
OR b.ADJTYPE = 'RSBDT'
OR b.ADJTYPE = 'TMIOW'
OR b.ADJTYPE = 'TRNSF'
OR b.ADJTYPE = 'WESTC')
--and b.ADJTYPE not in (select CODE from ADJTYPE where GLACCT &lt;&gt; '4900')
--and b.ADJTYPE &lt;&gt; 'TRNSF'
ORDER BY LTRIM(RTRIM(b.DATEBILLED))</property>
