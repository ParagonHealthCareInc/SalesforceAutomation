                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">Declare @StartDate datetime;
Declare @EndDate datetime;

--Previous Week
SET @StartDate = DATEADD(dd, -1, DATEADD(ww, DATEDIFF(ww, 0,GETDATE()) - 1, 0));
set @EndDate = DATEADD(dd, 5, DATEADD(ww, DATEDIFF(ww, 0, GETDATE()) - 1, 0));


Declare @DTC as table
	(EMR_system varchar (5),
	 AEBasesite varchar (max),
	 P_Site varchar (max),
	 AEMerged varchar (max),
	 CPR_ID varchar (max),
	 Referral_ID varchar (max),
	 Pat_ID varchar (max),
	 RefDate Datetime,
	 SOC Datetime,
	 Presc_ID varchar (max),
	 Therapy varchar (max),
	 Ref_Sourc varchar (max),
	 Name varchar (max),
	 Pat_Stat varchar (max),
	 DT_RXCode varchar (max),
	 DT int,
	 AE varchar (max),
	 Pri_Ins varchar (max),
	 Sec_Ins Varchar (max) null,
	 LastactiveDate Datetime)

Insert Into @DTC

Select distinct 'CPR+' AS EMR_System__c,
	xref.AEBaseSite,
    case when isnull(xref.site,'')='' then'&lt;No Site&gt;' else Xref.Site end Paragon_Site__c,
    Xref.Referral_Rep as SalesRepMerged,
	concat('CPR',' ',ltrim(rtrim(h.cpk_hr))) CPR_ID,
	CONCAT(h.MRN, ' : ', CONVERT(VARCHAR, h.REF_DATE, 111)) AS Referral_ID__c,
	convert(nvarchar,LTRIM(RTRIM(h.MRN))) AS Patient__c,
	CONVERT(VARCHAR, h.REF_DATE, 111) AS Referral_Date__c,
	CONVERT(VARCHAR, h.SOC, 111) AS SOC__c,
	LTRIM(RTRIM(a.PH_NPI)) Prescriber__c,
	CASE		WHEN LEN(LTRIM(RTRIM(h.CM))) = 0 THEN ''
		WHEN SUBSTRING(h.CM, 1, 1) = '.' THEN LTRIM(RTRIM(SUBSTRING(h.CM, CHARINDEX('.', h.CM) + 1, LEN(h.CM))))
		ELSE h.CM END AS Therapy__c
	, ltrim(rtrim(rs.org)) as Ref_source
	, concat(ltrim(rtrim(h.last_name)),', ', ltrim(rtrim(h.first_name))) Name
	, h.PAT_STAT, ltrim(rtrim(dt.RXCODE)) DT_RXCode, 
	min(dt.TICKNO) as DT, h.salesrep,
	IsNull(i.INSURANCE, dti.ORG) as Insurance, IsNull(i2.INSURANCE,'') as Insurance2,
(Select top 1 STAT_DATE from [SVRCPRDB03\CPR].CPR7.DBO.PTDAYS with (nolock) where MRN = h.MRN and NEW_STAT = 'Inactive' and  Convert(date, STAT_DATE, 101)  &lt; h.SOC 
order by STAT_DATE desc) as LastInactiveDate


from  [SVRCPRDB03\CPR].CPR7.DBO.TICKCI dt with (nolock) 
inner join [SVRCPRDB03\CPR].CPR7.DBO.HR h with(nolock) on dt.MRN = h.MRN
left join [SVRCPRDB03\CPR].CPR7.DBO.POPUPDATA PD with (nolock) on h.CFK_POPUPDATA_ENTTEAMS = CPK_POPUPDATA   
LEFT JOIN  [SVRCPRDB03\CPR].CPR7.DBO.PATINS i with (nolock) on i.MRN  = h.MRN and i.[STATUS] = 'Active' and i.DELFLAG = 0 and i.[RANK] = 1.0
LEFT JOIN  [SVRCPRDB03\CPR].CPR7.DBO.PATINS i2 with (nolock) on i2.MRN  = h.MRN and i2.[STATUS] = 'Active' and i2.DELFLAG = 0 and i2.[RANK] = 2.0
left join [SVRCPRDB03\CPR].CPR7.DBO.INSCOMP dti with (nolock) on dti.[NO] = dt.INSNO
left join (Select distinct salescode, site, referral_Rep, AEBaseSite from [DWDBHA].XREFDATA.dbo.SALESREPS with (nolock)
				where dbsource='cpr' and salescode is not null and active=1) Xref on Xref.salescode=h.salescode
inner join [SVRCPRDB03\CPR].CPR7.DBO.DOCTORS a on a.no= h.ph_no and a.delflag=0
inner join [DWDBHA].SALESFORCE.Staging.[user] au on au.name=xref.referral_Rep and au.isactive='1'-- check against active cpr owners from salesforce
inner join [DWDBHA].SALESFORCE.Staging.Prescribers sp on (sp.NPI__c = a.PH_NPI)--check against prescriber in SF
left join [SVRCPRDB03\CPR].CPR7.DBO.Roster rs with (nolock) on h.cfk_refsourc=rs.cpk_roster and rs.delflag=0 and rs.REFSRC=1
where dt.DELFLAG = 0
 and dt.PICKUP = 0

 --and (dt.DELIVDATE between @StartDate and @EndDate or dt.START between @StartDate and @EndDate)
 and dt.touchdate between @StartDate and @EndDate
---and h.SOC &gt;= @StartDate and h.SOC &lt;= @EndDate
 and dt.RXCODE &lt;&gt; 'NURSING'
 and a.PH_NPI &lt;&gt; ''
 and IsNull(PD.TEXT_,'') &lt;&gt; 'HEMOPHILIA'
group by PD.TEXT_, h.SALESREP, h.SALESCODE, h.FIRST_NAME, h.LAST_NAME, h.MRN, h.CM, h.REF_DATE, h.SOC, h.PAT_STAT, dt.RXCODE, IsNull(i.INSURANCE, dti.ORG), i2.INSURANCE,h.SERVAREA, xref.site, xref.salescode, xref.referral_rep, xref.AEBaseSite, a.PH_NPI, h.cpk_hr, rs.org, a.PH_SPEC;

 Declare @DTC_Orders as Table
	(Tickno int,
	 OrderNo Int)
 
 Insert Into @DTC_Orders

 Select TICKNO, Min(OTNO) as OrderNo
 
 from [SVRCPRDB03\CPR].CPR7.DBO.TICKC dtl with (nolock)
 where dtl.DELFLAG = 0
 and dtl.TICKNO in (select distinct DT from @DTC)
 group by TICKNO;

 select *
 from
 (

  Select t.*, OT.ITEMNAME, convert(Date,OT.START) as OrderStart, convert(Date,OT.[STOP]) as OrderStop,  
case when isnull(t.LastactiveDate,'')='' then ''
				when datediff(Month, t.LastactiveDate,t.soc) &gt; 6 then 'Restart' else '&lt; 6m' end Rs, 
isnull(sl.saleslevel, 'Other') saleslevel, sl.therapy tp, ot.rxtype ot_therapy
				, case when sl.saleslevel in ('Level 1','Level 2') then 'Special Therapy'--only level 1 and 2
						when isnull(sl.saleslevel,'')='' then 'Others'
						else 'Non Specialty Therapy' end [Therapy Type]
				, case when ot.rxtype in ('TPN', 'INOT', 'ANTI-INF') then 'C1/C2' else 'Others' end [Therapy Level]
				, CONCAT(ot.MRN, ' : ', O.OrderNo) RFID, 
				row_number() over(partition by ot.[no] order by ot.ordered desc) rd
 from @DTC t
 inner join @DTC_Orders o on t.DT = o.TICKNO
 inner join [SVRCPRDB03\CPR].CPR7.DBO.OT with (nolock) on OT.[NO] = o.OrderNo
 left join [DWDBHA].XREFDATA.DBO.[Xreftherapy] sl on sl.Therapy=ot.RXTYPE
  where  isnull(AEBaseSite,'')  not in ('SPECIALTY','HEMO') 
  and (case when isnull(t.LastactiveDate,'')='' then ''
				when datediff(Month, t.LastactiveDate,t.soc) &gt; 6 then 'Restart' else '&lt; 6m' end) &lt;&gt; '&lt; 6m'
--and ot.Ordered between @StartDate and @EndDate
and (convert(Date,ot.Touchdate) between @StartDate and @EndDate)
) a
where a.rd = 1
order by a.Name</property>
