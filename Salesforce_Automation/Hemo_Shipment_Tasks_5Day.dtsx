          <components>
            <component
              refId="Package\Data Flow Task\ADO NET Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT
	HR.MRN AS MRN
   ,CONCAT(LTRIM(RTRIM(HR.FIRST_NAME)), ' ', LTRIM(RTRIM(HR.LAST_NAME))) AS Name
   ,(SELECT
			MAX(l.NEXTDELIV)
		FROM [svrcprdb03\cpr].[CPR7].[dbo].LABELS l
		WHERE l.LINK = HR.MRN)
	AS NextDelivDate
   ,DATEADD(DAY, -5, (SELECT
			MAX(l.NEXTDELIV)
		FROM [svrcprdb03\cpr].[CPR7].[dbo].LABELS l
		WHERE l.LINK = HR.MRN)
	) AS DueDate --= (5 Days Prior or 3 Days Prior)
	--, dateadd(day,-3,( SELECT
	--		MAX(l.NEXTDELIV)
	--	FROM [CPRDBHA].[CPR7].[dbo].LABELS l
	--	WHERE l.LINK = HR.MRN)) as ReminderDate --= (5 Days Prior or 3 Days Prior)
   ,'Patient Interaction' AS Type
   ,'Phone Call' AS Subject
   ,s.New_Rep AS AssignedTo --= Sales Rep assigned to Patient in CPR
   ,CONCAT('Next Delivery Date is in 5 days for Patient: ', CONCAT(LTRIM(RTRIM(HR.FIRST_NAME)), ' ', LTRIM(RTRIM(HR.LAST_NAME))), '. Please call patient to schedule the delivery.') AS Comments
   ,'High' AS Priority
   ,'Open' AS Status
FROM [svrcprdb03\cpr].[CPR7].[dbo].HR
INNER JOIN [XREFDATA].[dbo].SalesReps s
	ON HR.SALESCODE = s.SALESCODE
WHERE CONVERT(VARCHAR, GETDATE(), 101) = CONVERT(VARCHAR, DATEADD(DAY, -5, (SELECT
		MAX(l.NEXTDELIV)
	FROM [svrcprdb03\cpr].[CPR7].[dbo].LABELS l
	WHERE l.LINK = HR.MRN)
), 101)
AND s.Division = 'HEMO'</property>
