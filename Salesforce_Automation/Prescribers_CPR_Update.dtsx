              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @start datetime;
Declare @end datetime;

--Previous Week
SET @start = DATEADD(dd, -1, DATEADD(ww, DATEDIFF(ww, 0, GETDATE()) - 1, 0));
SET @end = DATEADD(dd, 5, DATEADD(ww, DATEDIFF(ww, 0, GETDATE()) - 1, 0));

DECLARE
	@xrefTerritory TABLE (
		ZipCode VARCHAR(5)
	   ,AreaCode VARCHAR(10)
	   ,HITDistrict VARCHAR(10)
	   ,HITTerritory VARCHAR(10)
	   ,HITSalesRep VARCHAR(50)
	   ,IVCDistrict VARCHAR(10)
	   ,IVCTerritory VARCHAR(50)
	   ,SalesRep VARCHAR(50)
	   ,State VARCHAR(2)
	   ,HEMOTerritory VARCHAR(10)
	   ,HEMOSalesRep VARCHAR(50)
	   ,Inactive BIT
	   ,CreatedDate DATETIME
	   ,ModifiedDate DATETIME
	)
INSERT INTO @xrefTerritory
	SELECT
	ZipCode
   ,AreaCode
   ,HITDistrict
   ,HITTerritory
   ,HITSalesRep
   ,IVCDistrict
   ,IVCTerritory
   ,SalesRep
   ,State
   ,HEMOTerritory
   ,HEMOSalesRep
   ,Inactive
   ,CreatedDate
   ,ModifiedDate
	FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].XrefTerritoryMgmt
	WHERE Inactive = 0

SELECT
	LTRIM(RTRIM(d.PH_NPI)) AS [NPI__c]
   --,UPPER(CONCAT(LTRIM(RTRIM(d.PH_FIRST)), ' ', LTRIM(RTRIM(d.PH_LAST)))) AS [Name]
   --,LTRIM(RTRIM(d.PH_ADDRESS)) AS [BillingStreet__c]
   --,LTRIM(RTRIM(d.PH_CITY)) AS [BillingCity__c]
   --,LTRIM(RTRIM(d.PH_STATE)) AS BillingState__c
   --,SUBSTRING(LTRIM(RTRIM(d.PH_ZIP)), PATINDEX('%[0-9]%', LTRIM(RTRIM(d.PH_ZIP))), 5) AS [BillingPostalCode__c]
   ,UPPER(LTRIM(RTRIM(d.PH_SPEC))) AS [Specialty__c]
   --,d.PH_PHONE AS [Phone__c]
   --,d.PH_FAX AS [Fax__c]
   --,d.PROFDESIG AS [Designation__c]
, CASE 
	WHEN SUBSTRING(LTRIM(RTRIM(d.PH_ZIP)), PATINDEX('%[0-9]%', LTRIM(RTRIM(d.PH_ZIP))), 5) in (SELECT DISTINCT ZipCode FROM @xrefTerritory WHERE HITDistrict IS NOT NULL and LEN(LTRIM(RTRIM(ZipCode))) &gt; 0)
		THEN (SELECT HITDistrict FROM @xrefTerritory WHERE HITDistrict IS NOT NULL and ZipCode = SUBSTRING(LTRIM(RTRIM(d.PH_ZIP)), PATINDEX('%[0-9]%', LTRIM(RTRIM(d.PH_ZIP))), 5))
	WHEN (SELECT HITDistrict FROM @xrefTerritory WHERE AreaCode = SUBSTRING(d.PH_PHONE, 1, 3) AND d.INACTIVE = 0) IS NOT NULL THEN (SELECT HITDistrict FROM @xrefTerritory WHERE AreaCode = SUBSTRING(d.PH_PHONE, 1, 3) AND d.INACTIVE = 0)
	ELSE 'UNK-Unassigned'
	END AS [HIT_District__c]
, CASE 
	WHEN SUBSTRING(LTRIM(RTRIM(d.PH_ZIP)), PATINDEX('%[0-9]%', LTRIM(RTRIM(d.PH_ZIP))), 5) in (SELECT DISTINCT ZipCode FROM @xrefTerritory WHERE HITDistrict IS NOT NULL and LEN(LTRIM(RTRIM(ZipCode))) &gt; 0)
		THEN (SELECT HITTerritory FROM @xrefTerritory WHERE HITDistrict IS NOT NULL and ZipCode = SUBSTRING(LTRIM(RTRIM(d.PH_ZIP)), PATINDEX('%[0-9]%', LTRIM(RTRIM(d.PH_ZIP))), 5))
	WHEN (SELECT HITTerritory FROM @xrefTerritory WHERE AreaCode = SUBSTRING(d.PH_PHONE, 1, 3) AND d.INACTIVE = 0) IS NOT NULL THEN (SELECT HITTerritory FROM @xrefTerritory WHERE AreaCode = SUBSTRING(d.PH_PHONE, 1, 3) AND d.INACTIVE = 0)
	ELSE 'UNK-Unassigned-Unassigned'
	END AS [HIT_Territory__c]
   ,d.NO AS [CPR_ID__c]
   ,'0011I000016Bh2PQAS' AS [Account__c] --Assign to the Acct Staging Admin account by default
FROM dbo.DOCTORS d WITH (NOLOCK)
LEFT JOIN dbo.SITES WITH (NOLOCK)
	ON SITES.SITENO = d.SITENO
		AND SITES.DELFLAG = 0
WHERE d.DELFLAG = 0
--AND d.CREATEDON &gt;= @start
--AND d.CREATEDON &lt;= @end
AND LEN(LTRIM(RTRIM(d.PH_NPI))) &gt; 0</property>
