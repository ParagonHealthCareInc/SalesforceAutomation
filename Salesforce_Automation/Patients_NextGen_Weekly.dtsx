SELECT
	--Patient Demo
	UPPER(CONVERT(NVARCHAR(50), p.person_id)) AS MRN__c
   ,UPPER(CONCAT(LTRIM(RTRIM(p.first_name)), ' ', LTRIM(RTRIM(p.last_name)))) AS [Name]
   ,UPPER(LTRIM(RTRIM(p.first_name))) AS First_Name__c
   ,UPPER(LTRIM(RTRIM(p.last_name))) AS Last_Name__c
   ,ISNULL(LTRIM(RTRIM(p.address_line_1)), '') AS Address__c
   ,ISNULL(LTRIM(RTRIM(p.address_line_2)), '') AS Address_Line_2__c
   ,ISNULL(LTRIM(RTRIM(p.city)), '') AS City__c
   ,ISNULL(LTRIM(RTRIM(p.state)), '') AS State__c
   ,ISNULL(SUBSTRING(LTRIM(RTRIM(p.zip)), PATINDEX('%[0-9]%', LTRIM(RTRIM(p.zip))), 5), '') AS Zip__c
   ,ISNULL(LTRIM(RTRIM(p.day_phone)), '') AS Phone__c
   ,ISNULL(CONVERT(DATE, CAST(p.date_of_birth AS DATE), 110), '') AS DOB__c
   ,CASE
		WHEN ((LTRIM(RTRIM(p.email_address)) LIKE '_%@__%.__%' AND
			LEN(LTRIM(RTRIM(p.email_address))) &gt; 0) AND
			SUBSTRING(p.email_address, 0, CHARINDEX('@', p.email_address)) NOT LIKE '%[^a-zA-Z0-9]%')
			THEN p.email_address
		ELSE ''
	END AS Email__c
	--, format(p.date_of_birth, 'yyyy-MM-dd', 'en-US') as DOB__c
   ,ISNULL(LTRIM(RTRIM(p.current_gender)), '') AS Sex__c
	--Order Info
   ,ISNULL(CONVERT(DATE, CAST((SELECT TOP 1
			po.Order_Date
		FROM PIVC_Orders_ po (NOLOCK)
		WHERE po.person_id = p.person_id
		ORDER BY po.Order_Date DESC)
	AS DATE), 110), '') AS Last_Referral_Date__c
   ,ISNULL((SELECT TOP 1
			po.Order_Status
		FROM PIVC_Orders_ po (NOLOCK)
		WHERE po.person_id = p.person_id
		ORDER BY po.Order_Date DESC)
	, 'Pending') AS Status__c
   ,ISNULL((SELECT TOP 1
			po.Order_Type
		FROM PIVC_Orders_ po (NOLOCK)
		WHERE po.person_id = p.person_id
		ORDER BY po.Order_Date DESC)
	, '') AS Therapy__c

	--Provider Info
   ,CASE
		WHEN (SELECT
					LTRIM(RTRIM(pm.national_provider_id))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
			IS NULL
			THEN '9999999999'
		WHEN LEN((SELECT
					LTRIM(RTRIM(pm.national_provider_id))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
			) &lt; 10
			THEN '9999999999'
		ELSE (SELECT
					LTRIM(RTRIM(convert(nvarchar, pm.national_provider_id)))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
	END AS Prescriber__c

	--System Info
   ,CASE
		WHEN (SELECT TOP 1
					SUBSTRING(lm.external_id, CHARINDEX('-', lm.external_id) + 1, LEN(lm.external_id))
				FROM PIVC_Orders_ po
				LEFT JOIN location_mstr lm
					ON po.Service_Location = lm.location_name
				WHERE po.person_id = p.person_id)
			IS NULL
			THEN 'Not Assigned'
		ELSE (SELECT TOP 1
					SUBSTRING(lm.external_id, CHARINDEX('-', lm.external_id) + 1, LEN(lm.external_id))
				FROM PIVC_Orders_ po (NOLOCK)
				LEFT JOIN location_mstr lm
					ON po.Service_Location = lm.location_name
				WHERE po.person_id = p.person_id)
	END AS Paragon_Site__c
   ,'NextGen' AS EMR_System__c
   , concat('NG',' ', ltrim(rtrim(p.person_nbr))) [NextGen ID]
	,CASE
		--Look for AE on assigned Prescribers
		WHEN (SELECT
					NPI__c
				FROM @ActivePrescriber
				WHERE  NPI__c = (SELECT
										LTRIM(RTRIM(pm.national_provider_id))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id))
			=  (SELECT
					LTRIM(RTRIM(pm.national_provider_id))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
			THEN (SELECT
					CASE WHEN OwnerID IS NULL THEN '0051I000002GjcjQAC' ELSE OwnerID End
					FROM @ActivePrescriber
					WHERE NPI__c = (SELECT
							LTRIM(RTRIM(pm.national_provider_id))
						FROM provider_mstr pm (NOLOCK)
						WHERE (SELECT TOP 1
								po.Ordering_Provider_id
							FROM PIVC_Orders_ po (NOLOCK)
							WHERE po.person_id = p.person_id
							ORDER BY po.Order_Date DESC)
						= pm.provider_id))
		--Look for assigned Zip Code
		WHEN (SELECT
					x.zipcode
				FROM @TerritoryMgmt x
				LEFT JOIN @ActiveUsers u
					ON x.SALESREP = u.NAME
				WHERE x.[IVCTerritory] IS NOT NULL
				AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
				AND u.IsActive = '1')
			= LEFT((SELECT
						LTRIM(RTRIM(pm.zip))
					FROM provider_mstr pm (NOLOCK)
					WHERE (SELECT TOP 1
							po.Ordering_Provider_id
						FROM PIVC_Orders_ po (NOLOCK)
						WHERE po.person_id = p.person_id
						ORDER BY po.Order_Date DESC)
					= pm.provider_id), 5)
			THEN (SELECT
						CASE WHEN u.id IS NULL THEN '0051I000002GjcjQAC' ELSE U.ID END
					FROM @TerritoryMgmt x
					LEFT JOIN @ActiveUsers u
						ON x.SALESREP = u.NAME
					WHERE x.[IVCTerritory] IS NOT NULL
					AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
					AND u.IsActive = '1')
		--Default to Acct House
		ELSE '0051I000002GjcjQAC'
	END AS OwnerID
	,CASE
		--Look for AE on assigned Prescribers
		WHEN ( SELECT
			NPI__c
		FROM @ActivePrescriber
		WHERE NPI__c = (SELECT
				LTRIM(RTRIM(pm.national_provider_id))
			FROM provider_mstr pm (NOLOCK)
			WHERE (SELECT TOP 1
					po.Ordering_Provider_id
				FROM PIVC_Orders_ po (NOLOCK)
				WHERE po.person_id = p.person_id
				ORDER BY po.Order_Date DESC)
			= pm.provider_id))
			=  ( SELECT
			LTRIM(RTRIM(pm.national_provider_id))
		FROM provider_mstr pm (NOLOCK)
		WHERE (SELECT TOP 1
				po.Ordering_Provider_id
			FROM PIVC_Orders_ po (NOLOCK)
			WHERE po.person_id = p.person_id
			ORDER BY po.Order_Date DESC)
		= pm.provider_id)
			THEN ( SELECT
			CASE WHEN Sales_Rep_IVC__c IS NULL THEN '0051I000002GjcjQAC' ELSE Sales_Rep_IVC__c END
		FROM @ActivePrescriber
		WHERE NPI__c = (SELECT
				LTRIM(RTRIM(pm.national_provider_id))
			FROM provider_mstr pm (NOLOCK)
			WHERE (SELECT TOP 1
					po.Ordering_Provider_id
				FROM PIVC_Orders_ po (NOLOCK)
				WHERE po.person_id = p.person_id
				ORDER BY po.Order_Date DESC)
			= pm.provider_id))
		--Look for assigned Zip Code
		WHEN ( SELECT
			x.ZipCode
		FROM @TerritoryMgmt x
		LEFT JOIN @ActiveUsers u
			ON x.SalesRep = u.Name
		WHERE x.[IVCTerritory] IS NOT NULL
		AND x.ZipCode = LEFT((SELECT
				LTRIM(RTRIM(pm.zip))
			FROM provider_mstr pm (NOLOCK)
			WHERE (SELECT TOP 1
					po.Ordering_Provider_id
				FROM PIVC_Orders_ po (NOLOCK)
				WHERE po.person_id = p.person_id
				ORDER BY po.Order_Date DESC)
			= pm.provider_id)
		, 5)
		AND u.IsActive = '1')
			= LEFT(( SELECT
			LTRIM(RTRIM(pm.zip))
		FROM provider_mstr pm (NOLOCK)
		WHERE (SELECT TOP 1
				po.Ordering_Provider_id
			FROM PIVC_Orders_ po (NOLOCK)
			WHERE po.person_id = p.person_id
			ORDER BY po.Order_Date DESC)
		= pm.provider_id), 5)
			THEN ( SELECT
			CASE WHEN u.ID IS NULL THEN '0051I000002GjcjQAC' ELSE U.ID end
		FROM @TerritoryMgmt x
		LEFT JOIN @ActiveUsers u
			ON x.SalesRep = u.Name
		WHERE x.[IVCTerritory] IS NOT NULL
		AND x.ZipCode = LEFT((SELECT
				LTRIM(RTRIM(pm.zip))
			FROM provider_mstr pm (NOLOCK)
			WHERE (SELECT TOP 1
					po.Ordering_Provider_id
				FROM PIVC_Orders_ po (NOLOCK)
				WHERE po.person_id = p.person_id
				ORDER BY po.Order_Date DESC)
			= pm.provider_id)
		, 5)
		AND u.IsActive = '1')
		--Default to Acct House
		ELSE '0051I000002GjcjQAC'
	END AS Sales_Rep_IVC__c
FROM person p (NOLOCK)
INNER JOIN patient p1
	ON p.person_id = p1.person_id
WHERE p.person_id NOT IN ('EF477608-5F49-4159-A405-DB07E13EE7E4', '1EAA6B96-D42A-454F-86B4-BC669B3F6DFE', '1018C999-1B03-4B98-9A83-3FE166776450', '658921E3-C41F-46FE-A77C-B215A0F2CC75', 'D1C7E60B-F23F-477E-BCB4-7C3A2681B2EC', 'DCAF9281-84BC-4547-ABEC-68F68699110C')
