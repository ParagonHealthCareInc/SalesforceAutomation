              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE
	@start DATETIME;
DECLARE
	@end DATETIME;

--30 Min Automation
--SET @start = DATEADD(MINUTE, -31, GETDATE());
--SET @end = GETDATE();

--Previous Day
--SET @start = DATEADD(DAY, DATEDIFF(DAY, 2, GETDATE()), 0);
--SET @end = DATEADD(DAY, DATEDIFF(DAY, 0, GETDATE()), 0);

--Previous Week Automation
--SET @start = DATEADD(dd, -1, DATEADD(ww, DATEDIFF(ww, 0, GETDATE()) - 1, 0));
--SET @end = DATEADD(dd, 5, DATEADD(ww, DATEDIFF(ww, 0, GETDATE()) - 1, 0));

SET @start = '2021-01-01'
SET @end = GetDate()



DECLARE
	@ActiveUsers AS TABLE (
		ID nvarchar(18)
	   ,Name nvarchar(121)
	   ,IsActive bit
	   ,Sales_District__c varchar(4000)
	   ,Sales_Territory__c varchar(4000)
	)
INSERT INTO @ActiveUsers
SELECT Id, Name, IsActive, Sales_District__c, Sales_Territory__c 
FROM [CLUSTERSSRS\REPORTSERVICES].SALESFORCE.Staging.[User] u 
WHERE u.IsActive = '1' 
AND u.Sales_District__c LIKE '%IVC%'
AND u.Sales_Territory__c IS NOT null

--Optimized!
SELECT
	po.seq_no AS Referral_ID__c
   ,UPPER(po.person_id) AS Patient__c
   ,UPPER(CONCAT(LTRIM(RTRIM(p.first_name)), ' ', LTRIM(RTRIM(p.last_name)))) AS Name
   ,ISNULL(CONVERT(DATE, CAST(po.create_timestamp AS DATE), 101), '') AS Last_Referral_Date__c
   ,po.Order_Status AS Status__c
   ,CASE
		WHEN LTRIM(RTRIM(pm.national_provider_id)) IS NULL
			THEN '9999999999'
		WHEN LEN(LTRIM(RTRIM(pm.national_provider_id))) &lt; 10
			THEN '9999999999'
		ELSE LTRIM(RTRIM(pm.national_provider_id))
	END AS Prescriber__c
   ,po.Order_Type AS Therapy__c
   ,ISNULL((SELECT TOP 1
			ISNULL(SalesLevel, 'Others')
		FROM MLTS_MedInfoXP_ mx
		WHERE mx.ActiveInd = 'Y'
		AND mx.DrugName = po.Medication1
		ORDER BY ISNULL(mx.SalesLevel, '') DESC)
	, 'Others') AS NextGen_Sales_Level__c
	,CASE
		WHEN (SELECT	
			CASE 
				WHEN p1.IVC_Territory__c IS NULL THEN '0051I000002GjcjQAC' 
				ELSE (SELECT ID FROM @ActiveUsers WHERE Sales_Territory__c = p1.IVC_Territory__c)
			END AS Sales_Rep_IVC__c
		FROM [CLUSTERSSRS\REPORTSERVICES].[SALESFORCE].Staging.Prescribers p1
		WHERE p1.NPI__c = (SELECT
							LTRIM(RTRIM(pm.national_provider_id))
						FROM provider_mstr pm (NOLOCK)
						WHERE (SELECT TOP 1
								po.Ordering_Provider_id
							FROM PIVC_Orders_ po (NOLOCK)
							WHERE po.person_id = p.person_id
							ORDER BY po.Order_Date DESC)
						= pm.provider_id)
		AND pm.delete_ind = 'N') IS NULL THEN '0051I000002GjcjQAC'
		ELSE (SELECT	
			CASE 
				WHEN p1.IVC_Territory__c IS NULL THEN '0051I000002GjcjQAC' 
				ELSE (SELECT ID FROM @ActiveUsers WHERE Sales_Territory__c = p1.IVC_Territory__c)
			END AS Sales_Rep_IVC__c
		FROM [CLUSTERSSRS\REPORTSERVICES].[SALESFORCE].Staging.Prescribers p1
		WHERE p1.NPI__c = (SELECT
							LTRIM(RTRIM(pm.national_provider_id))
						FROM provider_mstr pm (NOLOCK)
						WHERE (SELECT TOP 1
								po.Ordering_Provider_id
							FROM PIVC_Orders_ po (NOLOCK)
							WHERE po.person_id = p.person_id
							ORDER BY po.Order_Date DESC)
						= pm.provider_id)
		AND pm.delete_ind = 'N') 
	END AS OwnerID
   ,CASE
		WHEN (SELECT TOP 1
					SUBSTRING(lm.external_id, CHARINDEX('-', lm.external_id) + 1, LEN(lm.external_id))
				FROM PIVC_Orders_ p
				LEFT JOIN location_mstr lm
					ON p.Service_Location = lm.location_name
				WHERE p.Service_Location = po.Service_Location)
			IS NULL
			THEN 'Not Assigned'
		ELSE (SELECT TOP 1
					SUBSTRING(lm.external_id, CHARINDEX('-', lm.external_id) + 1, LEN(lm.external_id))
				FROM PIVC_Orders_ p
				LEFT JOIN location_mstr lm
					ON p.Service_Location = lm.location_name
				WHERE p.Service_Location = po.Service_Location)
	END AS Paragon_Site__c
   ,'NextGen' AS EMR_System__c
   ,CASE
		WHEN NOT EXISTS (SELECT
					*
				FROM PIVC_Orders_ ou
				WHERE ou.seq_no &lt;&gt; po.seq_no
				AND ou.person_id = po.person_id
				AND ou.Order_Type = po.Order_Type
				AND ou.Ordering_Provider_id = po.Ordering_Provider_id)
			THEN 1
		ELSE 0
	END AS NewOrder__c
FROM PIVC_Orders_ po
INNER JOIN person p
	ON po.person_id = p.person_id
INNER JOIN provider_mstr pm
	ON po.Ordering_Provider_id = pm.provider_id
WHERE po.person_id NOT IN ('EF477608-5F49-4159-A405-DB07E13EE7E4', '1EAA6B96-D42A-454F-86B4-BC669B3F6DFE', '1018C999-1B03-4B98-9A83-3FE166776450', '658921E3-C41F-46FE-A77C-B215A0F2CC75', 'D1C7E60B-F23F-477E-BCB4-7C3A2681B2EC', 'DCAF9281-84BC-4547-ABEC-68F68699110C')
AND (po.modify_timestamp &gt;= @start
AND po.modify_timestamp &lt;= @end)
--AND pm.national_provider_id IN ('1811191786','1871557454')

ORDER BY Paragon_Site__c</property>
