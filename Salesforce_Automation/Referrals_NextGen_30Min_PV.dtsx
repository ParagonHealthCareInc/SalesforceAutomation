										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id))
			=  (SELECT
					LTRIM(RTRIM(pm.national_provider_id))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
			THEN (SELECT
						CASE WHEN Sales_Rep_IVC__c IS NULL THEN '0051I000002GjcjQAC' ELSE Sales_Rep_IVC__c END
					FROM @ActivePrescriber
					WHERE NPI__c = (SELECT
							LTRIM(RTRIM(pm.national_provider_id))
