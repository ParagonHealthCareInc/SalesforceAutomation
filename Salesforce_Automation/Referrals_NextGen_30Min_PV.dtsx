		--Look for assigned Zip Code
		WHEN (SELECT
					x.zipcode
				FROM @TerritoryMgmt x
				LEFT JOIN @ActiveUsers u
					ON x.SALESREP = u.NAME
				WHERE x.[IVCTerritory] IS NOT NULL
				AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
				AND u.IsActive = '1')
			= LEFT((SELECT
						LTRIM(RTRIM(pm.zip))
					FROM provider_mstr pm (NOLOCK)
					WHERE (SELECT TOP 1
							po.Ordering_Provider_id
						FROM PIVC_Orders_ po (NOLOCK)
						WHERE po.person_id = p.person_id
						ORDER BY po.Order_Date DESC)
					= pm.provider_id), 5)
			THEN (SELECT
						CASE WHEN u.id IS NULL THEN '0051I000002GjcjQAC' ELSE U.ID End
					FROM @TerritoryMgmt x
					LEFT JOIN @ActiveUsers u
						ON x.SALESREP = u.NAME
					WHERE x.[IVCTerritory] IS NOT NULL
					AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
					AND u.IsActive = '1')
		--Default to Acct House
		ELSE '0051I000002GjcjQAC'
	END AS Sales_Rep_IVC__c
	,CASE
			WHEN (SELECT TOP 1
						loc.Location
					FROM @Location loc
					WHERE loc.refer_provider_id = pm.provider_id
					ORDER BY loc.appt_date)
				IS NULL
				THEN 'Not Assigned'
			ELSE (SELECT TOP 1
						loc.Location
					FROM @Location loc
					WHERE loc.refer_provider_id = pm.provider_id
					ORDER BY loc.appt_date)
		END AS Paragon_Site__c
   ,'NextGen' AS EMR_System__c
   , concat('NG',' ',ltrim(rtrim(po.seq_no))) NG_ID
   ,CASE
		WHEN NOT EXISTS (SELECT
					*
				FROM PIVC_Orders_ ou
				WHERE ou.seq_no &lt;&gt; po.seq_no
				AND ou.person_id = po.person_id
				AND ou.Order_Type = po.Order_Type
				AND ou.Ordering_Provider_id = po.Ordering_Provider_id)
			THEN 1
		ELSE 0
