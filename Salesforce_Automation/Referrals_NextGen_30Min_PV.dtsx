                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE
	@start DATETIME;
DECLARE
	@end DATETIME;

--30 Min Automation
SET @start = DATEADD(MINUTE, -31, GETDATE());
SET @end = GETDATE();



DECLARE
	@ActivePrescriber AS TABLE (
		Account__c VARCHAR(25)
	   ,NPI__c VARCHAR(15)
	   ,Name VARCHAR(50)
	   ,OwnerID VARCHAR(50)
	   ,BillingPostalCode__c VARCHAR(5)
	   ,IVC_District__c VARCHAR(50)
	   ,IVC_Territory__c VARCHAR(50)
	   ,Sales_Rep_IVC__c VARCHAR(50)
	)
INSERT INTO @ActivePrescriber
	SELECT
		Account__c
	   ,NPI__c
	   ,Name
	   ,OwnerID
	   ,BillingPostalCode__c
	   ,IVC_District__c
	   ,IVC_Territory__c
	   ,p.Sales_Rep_IVC__c
	FROM [CLUSTERSSRS\REPORTSERVICES].SALESFORCE.Staging.Prescribers p;

DECLARE
	@TerritoryMgmt AS TABLE (
		ZipCode VARCHAR(5)
	   ,AreaCode VARCHAR(3)
	   ,HITDistrict VARCHAR(50)
	   ,HITTerritory VARCHAR(50)
	   ,HITSalesRep VARCHAR(50)
	   ,IVCDistrict VARCHAR(50)
	   ,IVCTerritory VARCHAR(50)
	   ,SalesRep VARCHAR(50)
	   ,State VARCHAR(50)
	   ,HEMOTerritory VARCHAR(50)
	   ,HEMOSalesRep VARCHAR(50)
	   ,Inactive BIT
	   ,CreatedDate DATETIME
	   ,ModifiedDate DATETIME
	)
INSERT INTO @TerritoryMgmt
	SELECT
		ZipCode
	   ,AreaCode
	   ,HITDistrict
	   ,HITTerritory
	   ,HITSalesRep
	   ,IVCDistrict
	   ,IVCTerritory
	   ,SalesRep
	   ,State
	   ,HEMOTerritory
	   ,HEMOSalesRep
	   ,Inactive
	   ,CreatedDate
	   ,ModifiedDate
	FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].XrefTerritoryMgmt
	WHERE Inactive = 0;

DECLARE
	@ActiveUsers AS TABLE (
		ID NVARCHAR(18)
	   ,Name NVARCHAR(121)
	   ,IsActive BIT
	   ,Sales_District__c VARCHAR(4000)
	   ,Sales_Territory__c VARCHAR(4000)
	)
INSERT INTO @ActiveUsers
	SELECT
		ID
	   ,Name
	   ,IsActive
	   ,Sales_District__c
	   ,Sales_Territory__c
	FROM [CLUSTERSSRS\REPORTSERVICES].SALESFORCE.Staging.[User] u
	WHERE u.IsActive = '1'
	AND u.Sales_District__c LIKE '%IVC%'
	AND u.Sales_Territory__c IS NOT NULL;

DECLARE
	@Location AS TABLE (
		refer_provider_id VARCHAR(50)
	   ,appt_date DATE
	   ,Location VARCHAR(50)
	)
INSERT INTO @Location
SELECT
		a.refer_provider_id
	   ,a.appt_date
	   ,SUBSTRING(l.external_id, CHARINDEX('-', l.external_id) + 1, LEN(l.external_id)) AS [Location]
	FROM appointments a
	INNER JOIN location_mstr l
		ON l.location_id = a.location_id
	WHERE a.delete_ind = 'N';

--Optimized!
SELECT
	po.seq_no AS Referral_ID__c
   ,UPPER(po.person_id) AS Patient__c
   ,UPPER(CONCAT(LTRIM(RTRIM(p.first_name)), ' ', LTRIM(RTRIM(p.last_name)))) AS Name
   ,ISNULL(CONVERT(DATE, CAST(po.create_timestamp AS DATE), 101), '') AS Last_Referral_Date__c
   ,po.Order_Status AS Status__c
   ,CASE
		WHEN LTRIM(RTRIM(pm.national_provider_id)) IS NULL
			THEN '9999999999'
		WHEN LEN(LTRIM(RTRIM(pm.national_provider_id))) &lt; 10
			THEN '9999999999'
		ELSE LTRIM(RTRIM(pm.national_provider_id))
	END AS Prescriber__c
   ,po.Order_Type AS Therapy__c
   ,ISNULL((SELECT TOP 1
			ISNULL(SalesLevel, 'Others')
		FROM MLTS_MedInfoXP_ mx
		WHERE mx.ActiveInd = 'Y'
		AND mx.DrugName = po.Medication1
		ORDER BY ISNULL(mx.SalesLevel, '') DESC)
	, 'Others') AS NextGen_Sales_Level__c
	,CASE
		--Look for AE on assigned Prescribers
		WHEN (SELECT
					NPI__c
				FROM @ActivePrescriber
				WHERE  NPI__c = (SELECT
										LTRIM(RTRIM(pm.national_provider_id))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id))
			=  (SELECT
					LTRIM(RTRIM(pm.national_provider_id))
				FROM provider_mstr pm (NOLOCK)
				WHERE (SELECT TOP 1
						po.Ordering_Provider_id
					FROM PIVC_Orders_ po (NOLOCK)
					WHERE po.person_id = p.person_id
					ORDER BY po.Order_Date DESC)
				= pm.provider_id)
			THEN (SELECT
						CASE WHEN Sales_Rep_IVC__c IS NULL THEN '0051I000002GjcjQAC' ELSE Sales_Rep_IVC__c End
					FROM @ActivePrescriber
					WHERE NPI__c = (SELECT
							LTRIM(RTRIM(pm.national_provider_id))
						FROM provider_mstr pm (NOLOCK)
						WHERE (SELECT TOP 1
								po.Ordering_Provider_id
							FROM PIVC_Orders_ po (NOLOCK)
							WHERE po.person_id = p.person_id
							ORDER BY po.Order_Date DESC)
						= pm.provider_id))
		--Look for assigned Zip Code
		WHEN (SELECT
					x.zipcode
				FROM @TerritoryMgmt x
				LEFT JOIN @ActiveUsers u
					ON x.SALESREP = u.NAME
				WHERE x.[IVCTerritory] IS NOT NULL
				AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
				AND u.IsActive = '1')
			= LEFT((SELECT
						LTRIM(RTRIM(pm.zip))
					FROM provider_mstr pm (NOLOCK)
					WHERE (SELECT TOP 1
							po.Ordering_Provider_id
						FROM PIVC_Orders_ po (NOLOCK)
						WHERE po.person_id = p.person_id
						ORDER BY po.Order_Date DESC)
					= pm.provider_id), 5)
			THEN (SELECT
						CASE WHEN u.id IS NULL THEN '0051I000002GjcjQAC' ELSE U.ID End
					FROM @TerritoryMgmt x
					LEFT JOIN @ActiveUsers u
						ON x.SALESREP = u.NAME
					WHERE x.[IVCTerritory] IS NOT NULL
					AND x.zipcode = LEFT((SELECT
										LTRIM(RTRIM(pm.zip))
									FROM provider_mstr pm (NOLOCK)
									WHERE (SELECT TOP 1
											po.Ordering_Provider_id
										FROM PIVC_Orders_ po (NOLOCK)
										WHERE po.person_id = p.person_id
										ORDER BY po.Order_Date DESC)
									= pm.provider_id), 5)
					AND u.IsActive = '1')
		--Default to Acct House
		ELSE '0051I000002GjcjQAC'
	END AS OwnerID
	,CASE
		--Look for AE on assigned Prescribers
		WHEN (SELECT
