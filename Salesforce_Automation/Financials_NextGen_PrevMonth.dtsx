            <component
              refId="Package\Data Flow Task\ADO NET Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE
	@StartDate DATETIME;
DECLARE
	@EndDate DATETIME;

--Previous Month
SET @StartDate = DATEADD(MONTH, DATEDIFF(MONTH, 0, GETDATE()) - 1, 0);
SET @EndDate = DATEADD(MONTH, DATEDIFF(MONTH, -1, GETDATE()) - 1, -1);

--SET @StartDate = '2020-07-01';
--SET @EndDate = '2020-07-31';

--select * from charges
SELECT
	a.enc_id AS 'Name'
   ,a.enc_nbr AS 'Invoice_Number__c'
   ,CONVERT(DATE, a.ProcessDate, 101) AS 'Date_Billed__c'
   ,LTRIM(RTRIM(a.person_id)) AS 'Patient__c'
   ,(TOTCHARGES + PayorAdj) AS 'Gross_Revenue__c'
   ,RevAdj AS 'Adjustments__c'
   ,((TOTCHARGES + PayorAdj) + RevAdj) AS 'Adjusted_Revenue__c'
   ,COGS AS 'COGS__c'
   ,(TOTCHARGES + PayorAdj + RevAdj - COGS) AS 'Gross_Profit__c'
   , --Check for nulls, then (Adjusted Revenue - Cost)
	ISNULL(a.PAYMENTS, 0) AS 'Payments__c'
   ,SUBSTRING(REPLACE(REPLACE(REPLACE(a.location_name, 'X Innovative Infusions N Richland Hills', 'Innovative Infusions Richland Hills'), 'Innovative Infusions 1 San Antonio', 'Innovative Infusions San Antonio'), 'X Innovative Infusions Knoxville', 'Innovative Infusions Knoxville'), 22, 50) AS Paragon_Site__c
   ,'NextGen' AS 'EMR__c'
   ,'IVC' AS 'Division__c'
   ,CASE
		WHEN (SELECT TOP 1
					New_Rep
				FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps]
				WHERE Active = 1
				AND DBSource = 'NEXTGEN'
				AND Location = a.location_name)
			IS NULL
			THEN 'ACCT HOUSE'
		ELSE (SELECT TOP 1
					New_Rep
				FROM [DWDBHA].[XREFDATA].[dbo].[SalesReps]
				WHERE Active = 1
				AND DBSource = 'NEXTGEN'
				AND Location = a.location_name)
	END AS OwnerID
FROM (SELECT
		pe.enc_id
	   ,CONVERT(DATETIME, chg.MinCloseDate) AS ProcessDate
	   ,pe.person_id
	   ,l.location_name
	   ,pe.enc_nbr
	   ,pe.enc_status
	   ,ISNULL(chg.total, 0) AS TOTCHARGES
	   ,ISNULL((SELECT
				SUM(ISNULL(td.adj_amt, 0))
			FROM dbo.trans_detail AS td
			INNER JOIN dbo.transactions t
				ON t.trans_id = td.trans_id
			LEFT JOIN dbo.tran_code_mstr tc
				ON tc.tran_code_id = t.tran_code_id
			LEFT JOIN dbo.charges c
				ON c.charge_id = td.charge_id
				AND c.link_id IS NULL
			WHERE (td.source_type = 'V')
			AND (td.post_ind = 'Y')
			AND t.type = 'A'
			AND tc.description IN ('Aetna Adjustment', 'BCBS Adjustment', 'Champus/Tricare Adjustment',
			'Cigna Adjustment', 'Commercial Adjustment', 'Humana Adjustment', 'Medicaid Adjustment',
			'Medicare Adjustment', 'Medicare Advantage Adjustment', 'UHC Adjustment',
			'Work Comp Adjustment')
			AND td.source_id = pe.enc_id)
		, 0) AS PayorAdj
	   ,ISNULL((SELECT
				SUM(ISNULL(td.adj_amt, 0))
			FROM dbo.trans_detail AS td
			INNER JOIN dbo.transactions t
				ON t.trans_id = td.trans_id
			LEFT JOIN dbo.tran_code_mstr tc
				ON tc.tran_code_id = t.tran_code_id
			LEFT JOIN dbo.charges c
				ON c.charge_id = td.charge_id
				AND c.link_id IS NULL
			WHERE (td.source_type = 'V')
			AND (td.post_ind = 'Y')
			AND t.type IN ('A', 'R')
			AND tc.description IN ('COAG', 'Interest Offset Adjustment', 'Bad Debt Final Write Off',
			'Bad Debt Insurance Adjustment', 'Bad Debt Recovery', 'Bad Debt SW', 'ZBad Debt Credit',
			'ZBad Debt Debit', 'Patient Payment Credit Card Ref', 'Patient Payment Refund',
			'REFPD  Refund Paid to Patient', 'REINS  Refund Paid to Insurance')
			AND td.source_id = pe.enc_id)
		, 0) AS NonRevAdj
	   ,CASE
			WHEN ISNUMERIC(REPLACE(REPLACE(pe.user_defined1, '$', ''), ',', '')) = 1
				THEN CONVERT(DECIMAL(10, 2), REPLACE(REPLACE(pe.user_defined1, '$', ''), ',', ''))
			ELSE 0.0
		END AS COGS
	   ,ISNULL((SELECT
				SUM(ISNULL(td.adj_amt, 0))
			FROM dbo.trans_detail AS td
			INNER JOIN dbo.transactions t
				ON t.trans_id = td.trans_id
			LEFT JOIN dbo.tran_code_mstr tc
				ON tc.tran_code_id = t.tran_code_id
			LEFT JOIN dbo.charges c
				ON c.charge_id = td.charge_id
				AND c.link_id IS NULL
			WHERE (td.source_type = 'V')
			AND (td.post_ind = 'Y')
			AND t.type IN ('A', 'R')
			AND tc.description NOT IN ('Aetna Adjustment', 'BCBS Adjustment', 'Champus/Tricare Adjustment',
			'Cigna Adjustment', 'Commercial Adjustment', 'Humana Adjustment', 'Medicaid Adjustment',
			'Medicare Adjustment', 'Medicare Advantage Adjustment', 'UHC Adjustment',
			'Work Comp Adjustment')
			AND tc.description NOT IN ('COAG', 'Interest Offset Adjustment', 'Bad Debt Final Write Off',
			'Bad Debt Insurance Adjustment', 'Bad Debt Recovery', 'Bad Debt SW', 'ZBad Debt Credit',
			'ZBad Debt Debit', 'Patient Payment Credit Card Ref', 'Patient Payment Refund',
			'REFPD  Refund Paid to Patient', 'REINS  Refund Paid to Insurance')
			AND td.source_id = pe.enc_id)
		, 0) AS RevAdj
	   ,ISNULL(trans.payment, 0) AS PAYMENTS
	FROM patient_encounter pe (NOLOCK)
	LEFT JOIN location_mstr l (NOLOCK)
		ON l.location_id = pe.location_id
	LEFT JOIN person p (NOLOCK)
		ON p.person_id = pe.person_id
	LEFT JOIN mstr_lists m (NOLOCK)
		ON m.mstr_list_item_id = pe.patient_type_id
	LEFT JOIN (SELECT
			pe.practice_id
		   ,pe.enc_id
		   ,MAX(closing_date) AS MaxCloseDate
		   ,MIN(closing_date) AS MinCloseDate
		   ,SUM(ISNULL(c.amt, 0)) AS total
		   ,SUM(ISNULL(c.cob1_amt, 0)) + SUM(ISNULL(c.cob2_amt, 0)) + SUM(ISNULL(c.cob3_amt, 0)) AS ins_amt
		   ,SUM(ISNULL(c.cob1_amt, 0)) AS cob1
		   ,SUM(ISNULL(c.cob2_amt, 0)) AS cob2
		   ,SUM(ISNULL(c.cob3_amt, 0)) AS cob3
		   ,SUM(ISNULL(c.pat_amt, 0)) AS pat
		   ,SUM(ISNULL(c.amt, 0)) + ISNULL((SELECT
					SUM(ISNULL(paid_amt, 0)) + SUM(ISNULL(adj_amt, 0)) AS Expr1
				FROM dbo.trans_detail AS td
				WHERE (pe.practice_id = practice_id)
				AND (pe.enc_id = source_id)
				AND (source_type = 'V')
				AND (post_ind = 'Y'))
			, 0) AS enc_bal
		FROM dbo.patient_encounter AS pe (NOLOCK)
		INNER JOIN dbo.charges AS c (NOLOCK)
			ON pe.practice_id = c.practice_id
			AND pe.enc_id = c.source_id
		WHERE (c.source_type = 'V')
		AND (pe.enc_status &lt;&gt; 'U')
		AND (pe.billable_ind = 'Y')
		AND c.link_id IS NULL
		GROUP BY pe.practice_id
				,pe.enc_id) AS chg
		ON chg.practice_id = pe.practice_id
		AND chg.enc_id = pe.enc_id
	LEFT JOIN encounter_payer ep (NOLOCK)
		ON ep.practice_id = pe.practice_id
		AND ep.enc_id = pe.enc_id
	LEFT JOIN payer_mstr pm (NOLOCK)
		ON pm.payer_id = ep.payer_id
	LEFT JOIN mstr_lists fin (NOLOCK)
		ON pm.financial_class = fin.mstr_list_item_id
	LEFT JOIN (SELECT
			td.practice_id
		   ,td.source_id AS enc_id
		   ,MIN(t.trans_id) AS trans_id
		   ,SUM(ISNULL(td.paid_amt, 0)) AS payment
		   ,SUM(ISNULL(td.adj_amt, 0)) AS adj
		FROM dbo.trans_detail AS td (NOLOCK)
		INNER JOIN dbo.transactions t (NOLOCK)
			ON t.trans_id = td.trans_id
		LEFT JOIN dbo.charges c (NOLOCK)
			ON c.charge_id = td.charge_id
			AND c.link_id IS NULL
		WHERE (td.source_type = 'V')
		AND (td.post_ind = 'Y') --and t.payer_id is not null
		GROUP BY td.practice_id
				,td.source_id) AS trans
		ON trans.practice_id = pe.practice_id
		AND trans.enc_id = pe.enc_id --and ep.payer_id = trans.payer_id
	WHERE billable_ind = 'Y'
	AND pe.enc_status &lt;&gt; 'U'
	AND ISNULL(ep.cob, 1) = 1) AS a
--where a.ProcessDate &gt;= '1/1/2018' and a.ProcessDate &lt;= '2/28/2019'
WHERE a.ProcessDate &gt;= @StartDate
AND a.ProcessDate &lt;= @EndDate
--where a.ProcessDate &gt;= @FromDate and a.ProcessDate &lt;= @ToDate
--and REVCODE in (@RevCode)
ORDER BY ProcessDate</property>
