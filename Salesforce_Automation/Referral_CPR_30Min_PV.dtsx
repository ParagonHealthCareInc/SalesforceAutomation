            <component
              refId="Package\Data Flow Task\ADO NET Source"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="ADO NET Source"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @start datetime;
Declare @end datetime;

--30 Min Automation
SET @start = CONVERT(VARCHAR, DATEADD(MINUTE, -31, GETDATE()), 20);
SET @end = CONVERT(VARCHAR, GETDATE(), 20);

SELECT
	
	CONCAT(h.MRN, ' : ', CONVERT(VARCHAR, h.REF_DATE, 111)) AS Referral_ID__c
   --,LTRIM(RTRIM(h.MRN)) AS Patient__c
   , convert(nvarchar,LTRIM(RTRIM(h.MRN))) AS Patient__c
   ,UPPER(CONCAT(LTRIM(RTRIM(h.FIRST_NAME)), ' ', LTRIM(RTRIM(h.LAST_NAME)))) AS Name
   ,CONVERT(VARCHAR, h.REF_DATE, 111) AS Referral_Date__c
   , convert(Date, h.ref_date) Rfdate
   , convert(Date, h.touchdate) RTDate
   ,CONVERT(VARCHAR, h.SOC, 111) AS SOC__c
   ,LTRIM(RTRIM(h.PAT_STAT)) AS Status__c
   ,CASE
		WHEN LTRIM(RTRIM(a.PH_NPI)) IS NULL
			THEN '9999999999'
		WHEN LEN(LTRIM(RTRIM(a.ph_NPI))) &lt; 10
			THEN '9999999999'
		ELSE LTRIM(RTRIM(a.PH_NPI))
	END AS Prescriber__c
   ,CASE
		WHEN LEN(LTRIM(RTRIM(h.CM))) = 0
			THEN ''
		WHEN SUBSTRING(h.CM, 1, 1) = '.'
			THEN LTRIM(RTRIM(SUBSTRING(h.CM, CHARINDEX('.', h.CM) + 1, LEN(h.CM))))
		ELSE h.CM
	END AS Therapy__c
   ,CASE
		WHEN (SELECT TOP 1
					PATINS.INSURANCE
				FROM PATINS
				WHERE DELFLAG = 0
				AND rank = 1
				AND PATINS.MRN = h.MRN)
			IS NULL
			THEN ''
		ELSE (SELECT TOP 1
					PATINS.INSURANCE
				FROM PATINS
				WHERE DELFLAG = 0
				AND rank = 1
				AND PATINS.MRN = h.MRN)
	END AS Primary_Insurance__c
   ,CASE
		WHEN (SELECT TOP 1
					PATINS.INSURANCE
				FROM PATINS
				WHERE DELFLAG = 0
				AND rank = 2
				AND PATINS.MRN = h.MRN)
			IS NULL
			THEN ''
		ELSE (SELECT TOP 1
					PATINS.INSURANCE
				FROM PATINS
				WHERE DELFLAG = 0
				AND rank = 2
				AND PATINS.MRN = h.MRN)
	END AS Secondary_Insurance__c
   ,CASE
		WHEN EXISTS (SELECT
					*
				FROM (SELECT
						b.SALESCODE
					FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].[SalesReps] b
					WHERE b.Active = '1') sr
				WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
			THEN CASE
					WHEN LTRIM(RTRIM(h.SALESREP)) LIKE '%House%'
						THEN 'ACCT HOUSE'
					ELSE (SELECT
								sr.New_Rep
							FROM (SELECT
									b.New_Rep
								   ,b.SALESCODE
								FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].[SalesReps] b
								WHERE b.Active = '1') sr
							WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
				END
                                          else ''
		--ELSE 'ACCT HOUSE' no Referral with no AE needs to be inserted
	END AS 'SALESREP'
   ,CASE
		WHEN EXISTS (SELECT
					*
				FROM (SELECT
						b.SALESCODE
					FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].[SalesReps] b
					WHERE b.Active = '1') sr
				WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
			THEN CASE
					WHEN LTRIM(RTRIM(h.SALESREP)) LIKE '%House%'
						THEN 'ACCT HOUSE'
					ELSE (SELECT
								CASE
									WHEN CHARINDEX('_', LTRIM(RTRIM(h.SALESREP))) &gt; 0
										THEN RTRIM(SUBSTRING(LTRIM(RTRIM(h.SALESREP)), 0, CHARINDEX('_', LTRIM(RTRIM(h.SALESREP)), 0)))
									WHEN CHARINDEX('-', LTRIM(RTRIM(h.SALESREP))) &gt; 0
										THEN RTRIM(SUBSTRING(LTRIM(RTRIM(h.SALESREP)), 0, CHARINDEX('-', LTRIM(RTRIM(h.SALESREP)), 0)))
									WHEN CHARINDEX('(', LTRIM(RTRIM(h.SALESREP))) &gt; 0
										THEN RTRIM(SUBSTRING(LTRIM(RTRIM(h.SALESREP)), 0, CHARINDEX('(', LTRIM(RTRIM(h.SALESREP)), 0)))
									ELSE LTRIM(RTRIM(h.SALESREP))
								END
							FROM (SELECT
									b.New_Rep
								   ,b.SALESCODE
								FROM [CLUSTERSSRS\REPORTSERVICES].[XREFDATA].[dbo].[SalesReps] b
								WHERE b.Active = '1') sr
							WHERE LTRIM(RTRIM(sr.SALESCODE)) = LTRIM(RTRIM(h.SALESCODE)))
				END
		ELSE ''
	END AS [Original_Sales_Rep__c]
   ,CASE
		WHEN LEN(LTRIM(RTRIM(s.SiteName))) &lt; 1 OR
			LTRIM(RTRIM(s.SiteName)) IS NULL
			THEN 'Not Assigned'
		ELSE LTRIM(RTRIM(s.SiteName))
	END AS 'Paragon_Site__c'
   ,'CPR+' AS EMR_System__c
   , concat('CPR',' ',ltrim(rtrim(h.cpk_hr))) CPR_ID
FROM HR h
LEFT JOIN (SELECT
		*
	FROM (SELECT
			ROW_NUMBER() OVER (PARTITION BY rl.CHILDKEY ORDER BY h.SALESFK DESC) ColSequence
		   ,rl.CHILDKEY
		   ,h.SALESFK
		   ,h.SALESREP
		   ,h.SALESCODE
		   ,d.*
		FROM RECORDLINKS rl
		LEFT JOIN DOCTORS d
			ON rl.CHILDKEY = d.NO
		LEFT JOIN (SELECT
				MRN
			   ,SALESREP
			   ,SALESCODE
			   ,SALESFK
			FROM HR
			WHERE MRN &lt;&gt; 'CASH') h
			ON rl.PARENTKEY = h.MRN
		WHERE rl.CHILDTABLE = 'DOCTORS'
		AND rl.CHILDKEY &lt;&gt; '-1') a
	WHERE a.ColSequence = '1') a
	ON h.PH_NO = a.NO
LEFT JOIN SITES s
	ON h.SITENO = s.SITENO
WHERE h.DELFLAG = 0
AND UPPER(CONCAT(LTRIM(RTRIM(h.FIRST_NAME)), ' ', LTRIM(RTRIM(h.LAST_NAME)))) NOT LIKE '%TEST%'
AND h.REF_DATE IS NOT NULL
AND LEN(LTRIM(RTRIM(h.REF_DATE))) &gt; 0
AND h.TOUCHDATE &gt;= @start
AND h.TOUCHDATE &lt;= @end</property>
